  <xs:simpleType name="VL1088_5_0_1_4">
    <xs:restriction base="xs:string">
      <xs:enumeration value="10000">
        <xs:annotation>
          <xs:documentation source="valueID">10000</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft</xs:documentation>
          <xs:documentation source="detailedDescription">Powered Fixed-wing aircraft.</xs:documentation>
          <xs:documentation source="explanation">This section covers flight phases specifically adopted for the operation of a powered fixed-wing aircraft.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10100">
        <xs:annotation>
          <xs:documentation source="valueID">10100</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing</xs:documentation>
          <xs:documentation source="detailedDescription">Standing.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight prior to pushback or taxi, or after arrival, at the gate, ramp, or parking area, while the aircraft is stationary.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10101">
        <xs:annotation>
          <xs:documentation source="valueID">10101</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Engine(s) Not Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Not Operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight, while the aircraft is standing and during which no aircraft engine is running.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10102">
        <xs:annotation>
          <xs:documentation source="valueID">10102</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Engine(s) Start-up</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Start-up.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight, while the aircraft is parked during which the first engine is started.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990899">
        <xs:annotation>
          <xs:documentation source="valueID">990899</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Engine(s) Run-up</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Run-up.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight after start-up, during which power is applied to engines, for a pre-flight engine performance test.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10103">
        <xs:annotation>
          <xs:documentation source="valueID">10103</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Engine(s) Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight following engine start-up, or after post-flight arrival at the destination.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10104">
        <xs:annotation>
          <xs:documentation source="valueID">10104</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Engine(s) Shut Down</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Shut Down.</xs:documentation>
          <xs:documentation source="explanation">Engine shutdown is from the start of the shutdown sequence until the engine(s) cease rotation.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10198">
        <xs:annotation>
          <xs:documentation source="valueID">10198</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Standing - Standing : Other</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any standing phase of flight other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990921">
        <xs:annotation>
          <xs:documentation source="valueID">990921</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Tow</xs:documentation>
          <xs:documentation source="detailedDescription">Tow</xs:documentation>
          <xs:documentation source="explanation"></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10200">
        <xs:annotation>
          <xs:documentation source="valueID">10200</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which movement of an aircraft on the surface of an aerodrome under its own power occurs, excluding take-off and landing. ICAO Annex 2.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10201">
        <xs:annotation>
          <xs:documentation source="valueID">10201</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Pushback</xs:documentation>
          <xs:documentation source="detailedDescription">Pushback</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when an aircraft is moving in the gate, ramp, or parking area, not under its own power, but assisted by a tow vehicle [tug].
Note: unassisted movement in the gate or ramp area is included in the taxiing phase.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990825">
        <xs:annotation>
          <xs:documentation source="valueID">990825</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Pushback - Assisted, Engine(s) Not Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Assisted, Engine(s) Not Operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when an aircraft is moved in the gate, ramp, or parking area, not under its own power, during which no engine is running.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990826">
        <xs:annotation>
          <xs:documentation source="valueID">990826</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Pushback - Assisted, Engine(s) Start-up</xs:documentation>
          <xs:documentation source="detailedDescription">Assisted, Engine(s) Start-up.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when an aircraft is moved in the gate, ramp, or parking area, not under its own power, from the time of the first engine startup sequence until last engine startup sequence is completed.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990827">
        <xs:annotation>
          <xs:documentation source="valueID">990827</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Pushback - Assisted, Engine(s) Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Assisted, Engine(s) Operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when an aircraft is moved in the gate, ramp, or parking area, not under its own power, after all engines have been started-up or before the shut-down sequence has been initiated.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990828">
        <xs:annotation>
          <xs:documentation source="valueID">990828</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Pushback - Assisted, Engine(s) Shutting Down</xs:documentation>
          <xs:documentation source="detailedDescription">Assisted, Engine(s) Shutting Down.</xs:documentation>
          <xs:documentation source="explanation">Engine shutdown is from the start of the shutdown sequence until the engine(s) cease rotation.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990914">
        <xs:annotation>
          <xs:documentation source="valueID">990914</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Power back</xs:documentation>
          <xs:documentation source="detailedDescription">Power back</xs:documentation>
          <xs:documentation source="explanation">The aircraft is reversing under it's own power from the parking position.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10202">
        <xs:annotation>
          <xs:documentation source="valueID">10202</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing to/from runway</xs:documentation>
          <xs:documentation source="detailedDescription">Taxiing to/from runway.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight, after reaching the movement area, when the aircraft progresses under its own power to the departure runway, or post-flight moves under its own power after leaving the landing runway.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990829">
        <xs:annotation>
          <xs:documentation source="valueID">990829</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing to/from runway - Taxi to runway</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi to runway.</xs:documentation>
          <xs:documentation source="explanation">Commences when the aircraft begins to move under its own power leaving the gate, ramp, apron, or parking area, and terminates upon reaching the runway.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990830">
        <xs:annotation>
          <xs:documentation source="valueID">990830</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing to/from runway - Taxi to take-off position</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi to take-off position.</xs:documentation>
          <xs:documentation source="explanation">From entering the runway until reaching the take-off position.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990831">
        <xs:annotation>
          <xs:documentation source="valueID">990831</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing to/from runway - Taxi from runway</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi from runway.</xs:documentation>
          <xs:documentation source="explanation">Begins upon exiting the landing runway and terminates upon arrival at the gate, ramp, apron, or parking area, when the aircraft ceases to move under its own power.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990832">
        <xs:annotation>
          <xs:documentation source="valueID">990832</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing to/from runway - Maintaining position</xs:documentation>
          <xs:documentation source="detailedDescription">Maintaining position.</xs:documentation>
          <xs:documentation source="explanation">Maintaining position at holding point.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990833">
        <xs:annotation>
          <xs:documentation source="valueID">990833</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - High speed taxi trial</xs:documentation>
          <xs:documentation source="detailedDescription">High speed taxi trial.</xs:documentation>
          <xs:documentation source="explanation">High speed taxi trial: taxiing on a runway/taxiway at high speed to test/check the aircraft.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10298">
        <xs:annotation>
          <xs:documentation source="valueID">10298</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Taxi - Taxiing: Other</xs:documentation>
          <xs:documentation source="detailedDescription">Taxiing: Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of taxiing other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10300">
        <xs:annotation>
          <xs:documentation source="valueID">10300</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Take-off.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the application of take-off power until reaching the first prescribed power reduction, or until reaching the VFR pattern or 1000 feet (300 metres) above runway end elevation, whichever comes first or the termination (abort) of the take-off.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above runway end elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10301">
        <xs:annotation>
          <xs:documentation source="valueID">10301</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Take-off run</xs:documentation>
          <xs:documentation source="detailedDescription">Take-off run.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the application of take-off power, through the take-off roll and rotation up to 35 feet [12 metres] above runway end elevation or until gear-up selection, whichever comes first.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10303">
        <xs:annotation>
          <xs:documentation source="valueID">10303</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Rejected take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Rejected take-off.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which any attempt is made to terminate a take-off between the application of take-off power, through rotation and up to 35 feet [or 12 metres] above the elevation of the runway end (from the point where the decision to abort has been taken until the aircraft begins to taxi from the runway).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10302">
        <xs:annotation>
          <xs:documentation source="valueID">10302</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Initial climb</xs:documentation>
          <xs:documentation source="detailedDescription">Initial climb.</xs:documentation>
          <xs:documentation source="explanation">From the end of the Takeoff run sub-phase to the first prescribed power reduction, or until reaching 1000 feet above runway elevation or the VFR pattern, whichever comes first.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above runway elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10304">
        <xs:annotation>
          <xs:documentation source="valueID">10304</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Climb into traffic pattern</xs:documentation>
          <xs:documentation source="detailedDescription">Climb into traffic pattern.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from 35 feet [12 metres] above runway end elevation to the first prescribed power reduction on reaching the VFR pattern. Applies to pilot training in which the aircraft's climb phase is not intended to reach altitude.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10305">
        <xs:annotation>
          <xs:documentation source="valueID">10305</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Emergency descent during take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency descent during take-off.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an intentional descent is made, in response to an emergency, which occurs after rotation but before the first prescribed power reduction, on reaching 1000 feet (300 metres) or the VFR pattern, whichever comes first.

Note: this slightly differs from the previous definition where reaching 1,500 feet (450 metres) was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10306">
        <xs:annotation>
          <xs:documentation source="valueID">10306</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Uncontrolled descent during take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Uncontrolled descent during take-off.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which any uncontrolled descent occurs after rotation but before the first prescribed power reduction, on reaching 1000 feet (300 metres) or the VFR pattern, whichever comes first.

Note: this slightly differs from the previous definition where reaching 1,500 feet (450 metres) was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10398">
        <xs:annotation>
          <xs:documentation source="valueID">10398</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Take-off - Take-off: Other</xs:documentation>
          <xs:documentation source="detailedDescription">Take-off: Other.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which any event occurs during take-off, other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10400">
        <xs:annotation>
          <xs:documentation source="valueID">10400</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route</xs:documentation>
          <xs:documentation source="detailedDescription">En-route.</xs:documentation>
          <xs:documentation source="explanation">Instrument Flight Rules (IFR): From completion of Initial Climb through cruise altitude and completion of controlled descent to the Initial Approach Fix (IAF). 

Visual Flight Rules (VFR): From completion of Initial Climb through cruise and controlled descent to the VFR pattern altitude or 1000 feet above runway elevation, whichever comes first.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above runway elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10401">
        <xs:annotation>
          <xs:documentation source="valueID">10401</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Climb to cruising level or altitude</xs:documentation>
          <xs:documentation source="detailedDescription">Climb to cruising level or altitude.</xs:documentation>
          <xs:documentation source="explanation">Climb to Cruise:
IFR: From completion of Initial Climb to arrival at initial assigned cruise altitude. 
VFR: From completion of Initial Climb to initial cruise altitude.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10402">
        <xs:annotation>
          <xs:documentation source="valueID">10402</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Cruise</xs:documentation>
          <xs:documentation source="detailedDescription">Cruise.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the top of climb to cruise altitude, or flight level, to the start of the descent toward the destination aerodrome or landing site.
Any level flight segment after arrival at initial cruise altitude until the start of descent to the destination.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10403">
        <xs:annotation>
          <xs:documentation source="valueID">10403</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Change of cruise level</xs:documentation>
          <xs:documentation source="detailedDescription">Change of cruise level..</xs:documentation>
          <xs:documentation source="explanation">The phase of flight during which the aircraft climbs, or descends, from one cruising flight level or altitude to the next cruising flight level or altitude.
Any climb or descent during cruise after the initial climb to cruise, but before descent to the destination.

Includes: cruise climb. An aeroplane cruising technique resulting in a net increase in altitude as the aeroplane mass decreases. ICAO DOC 4444</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10404">
        <xs:annotation>
          <xs:documentation source="valueID">10404</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Normal descent</xs:documentation>
          <xs:documentation source="detailedDescription">Normal descent.</xs:documentation>
          <xs:documentation source="explanation">Normal descent: 
IFR: Descent from cruise to either Initial Approach Fix (IAF) or VFR pattern entry. 
VFR: Descent from cruise to the VFR pattern entry or 1000 feet above the runway elevation, whichever comes first.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above the runway elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10405">
        <xs:annotation>
          <xs:documentation source="valueID">10405</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Emergency descent en-route</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency descent en-route.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an intentionally rapid, or premature, descent is made en-route, in response to an in-flight emergency. The descent is controlled by the crew.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10406">
        <xs:annotation>
          <xs:documentation source="valueID">10406</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - Uncontrolled descent en-route</xs:documentation>
          <xs:documentation source="detailedDescription">Uncontrolled descent en-route.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which the aircraft descends uncontrolled. </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10407">
        <xs:annotation>
          <xs:documentation source="valueID">10407</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - En-route - holding</xs:documentation>
          <xs:documentation source="detailedDescription">En-route - holding.</xs:documentation>
          <xs:documentation source="explanation">Execution of a predetermined maneuver (usually an oval race track pattern) which keeps the aircraft within a specified airspace while awaiting further clearance. 
Note: descent during holding is also covered in this sub-phase. A holding procedure executed at the IAF is also included in this sub-phase.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10498">
        <xs:annotation>
          <xs:documentation source="valueID">10498</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - En-route - En-route: Other</xs:documentation>
          <xs:documentation source="detailedDescription">En-route: Other.</xs:documentation>
          <xs:documentation source="explanation">Any phase of flight en-route, other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10500">
        <xs:annotation>
          <xs:documentation source="valueID">10500</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring</xs:documentation>
          <xs:documentation source="detailedDescription">Manoeuvring.</xs:documentation>
          <xs:documentation source="explanation">An event involving a phase of flight in which planned low-level flight, or attitude, or planned abnormal attitude, or abnormal acceleration occurs.
Low altitude/aerobatic flight operations</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10501">
        <xs:annotation>
          <xs:documentation source="valueID">10501</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring - Aerobatics</xs:documentation>
          <xs:documentation source="detailedDescription">Aerobatics.</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of flight in which manoeuvring into an abrupt change in attitude, abnormal attitude or abnormal acceleration occurs (usually associated with air shows and military flight).

Any intentional maneuvering that exceeds 30 degrees of pitch attitude or 60 degrees of bank, or both, or abnormal acceleration.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10502">
        <xs:annotation>
          <xs:documentation source="valueID">10502</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring - Low flying</xs:documentation>
          <xs:documentation source="detailedDescription">Low flying.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight made at a height below the normal minima, e.g. in preparation for, or during, observation work, demonstration, photography work, aerial application, training, sight seeing, ostentatious display, or other similar activity.

Intentional low-altitude flight not connected with a landing or takeoff.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10503">
        <xs:annotation>
          <xs:documentation source="valueID">10503</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring - Emergency descent</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency descent.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an intentionally rapid or premature descent, from a previously normal manoeuvre, is made in response to an in-flight emergency. The descent is controlled by the crew.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10504">
        <xs:annotation>
          <xs:documentation source="valueID">10504</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring - Uncontrolled descent</xs:documentation>
          <xs:documentation source="detailedDescription">Uncontrolled descent.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an uncontrolled descent from a previously normal manoeuvre occurs. </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10598">
        <xs:annotation>
          <xs:documentation source="valueID">10598</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Manoeuvring - Manoeuvring: Other</xs:documentation>
          <xs:documentation source="detailedDescription">Manoeuvring: Other</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of manoeuvring flight other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10600">
        <xs:annotation>
          <xs:documentation source="valueID">10600</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach</xs:documentation>
          <xs:documentation source="detailedDescription">Approach.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the outer marker to the to the point of transition from nose-low to nose-high attitude immediately prior to the flare above the runway [IFR]; or [VFR] from 1000 feet (300 metres) above the runway end elevation or from the point of VFR pattern entry to the flare above the runway.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above the runway end elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10611">
        <xs:annotation>
          <xs:documentation source="valueID">10611</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Initial Approach</xs:documentation>
          <xs:documentation source="detailedDescription">Initial Approach</xs:documentation>
          <xs:documentation source="explanation">Initial Approach (IFR): From the Initial Approach Fix (IAF) to the Final Approach Fix (FAF).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10601">
        <xs:annotation>
          <xs:documentation source="valueID">10601</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Intermediate approach</xs:documentation>
          <xs:documentation source="detailedDescription">Intermediate approach.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight between the middle approach fix and the final approach fix; or between the end of a reversal procedure or dead-reckoning track procedure and the final approach fix.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10602">
        <xs:annotation>
          <xs:documentation source="valueID">10602</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Final approach</xs:documentation>
          <xs:documentation source="detailedDescription">Final approach.</xs:documentation>
          <xs:documentation source="explanation">Final approach. that part of an instrument approach procedure which commences at the specified final approach fix or point, or where such a fix or point is not specified,
 
 a) at the end of the last procedure turn, base turn or inbound turn of a racetrack procedure, if specified; or
 
 b) at the point of interception of the last track specified in the approach procedure; and ends at a point in the vicinity of an aerodrome from which:
 
 1) a landing can be made; or
 2) a missed approach procedure is initiated.

 ICAO Annex 11</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10603">
        <xs:annotation>
          <xs:documentation source="valueID">10603</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Circuit pattern - downwind</xs:documentation>
          <xs:documentation source="detailedDescription">Circuit pattern - downwind.</xs:documentation>
          <xs:documentation source="explanation">The phase of VFR flight from 1000 feet above runway end elevation (300 metres) or the point of VFR pattern entry; commences at about 45 degrees from the threshold, continues parallel to the runway in the direction opposite to landing and terminates upon initiating the turn to base leg.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above runway end elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10604">
        <xs:annotation>
          <xs:documentation source="valueID">10604</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Circuit pattern - base leg</xs:documentation>
          <xs:documentation source="detailedDescription">Circuit pattern - base leg.</xs:documentation>
          <xs:documentation source="explanation">The phase of VFR flight from the start of the turn at end of downwind leg, then at 90 degrees to the landing runway until reaching the extended centre line off the approach end of the runway.
 Base turn: a turn executed by the aircraft during the initial approach between the end of the outbound track and the beginning of the intermediate or final approach track. The tracks are not reciprocal.
 Note: Base turns may be designated as being made either in level flight or while descending, according to the circumstances of each individual procedure.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10605">
        <xs:annotation>
          <xs:documentation source="valueID">10605</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Circuit pattern - final</xs:documentation>
          <xs:documentation source="detailedDescription">Circuit pattern - final approach.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight between the start of the turn from base leg to a flight path along the extended runway centre line, to the runway.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990898">
        <xs:annotation>
          <xs:documentation source="valueID">990898</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Circuit pattern - crosswind</xs:documentation>
          <xs:documentation source="detailedDescription">Circuit pattern - crosswind.</xs:documentation>
          <xs:documentation source="explanation">A flight path of the VFR traffic pattern, which is perpendicular to the landing runway, crosses the departure end of the runway, and connects with the downwind leg.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10606">
        <xs:annotation>
          <xs:documentation source="valueID">10606</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Aborted approach before decision height</xs:documentation>
          <xs:documentation source="detailedDescription">Aborted - interrupted approach (before reaching decision height).</xs:documentation>
          <xs:documentation source="explanation">The phase of flight where the pilot aborts the approach during an ILS or other precision approach involving an abnormally early turn on approach before reaching minimum decision altitude or decision height.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10607">
        <xs:annotation>
          <xs:documentation source="valueID">10607</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Missed approach or go-around</xs:documentation>
          <xs:documentation source="detailedDescription">Missed approach or go-around.</xs:documentation>
          <xs:documentation source="explanation">Missed Approach/Go-Around: From the first application of power after the crew elects to execute a missed approach or go-around until the aircraft re-enters the sequence for a VFR pattern (go-around) or until the aircraft reaches the IAF for another approach (IFR)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10608">
        <xs:annotation>
          <xs:documentation source="valueID">10608</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Emergency descent during approach</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency descent during approach.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an intentionally rapid or premature descent is made in response to an in-flight emergency during approach. The descent is controlled by the crew.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10609">
        <xs:annotation>
          <xs:documentation source="valueID">10609</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Uncontrolled descent during an approach</xs:documentation>
          <xs:documentation source="detailedDescription">Uncontrolled descent during an approach.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which a previously normal descent during an approach becomes uncontrolled.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10610">
        <xs:annotation>
          <xs:documentation source="valueID">10610</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Approach - holding</xs:documentation>
          <xs:documentation source="detailedDescription">Approach - holding.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which a pre-determined ad-hoc manoeuvre during the approach keeps the aircraft within a specified airspace awaiting further instructions.

Note: a holding procedure executed at the IAF is included in the En-route phase.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10698">
        <xs:annotation>
          <xs:documentation source="valueID">10698</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Approach - Approach : Other</xs:documentation>
          <xs:documentation source="detailedDescription">Approach : Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of flight, while on approach, in which an event other than one of the above occurs.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10700">
        <xs:annotation>
          <xs:documentation source="valueID">10700</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing</xs:documentation>
          <xs:documentation source="detailedDescription">Landing.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the point of transition from nose-low to nose-up attitude, immediately before landing (flare), through touchdown and until aircraft exits landing runway, comes to a stop or when power is applied for takeoff in the case of a touch-and-go landing, whichever occurs first.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10701">
        <xs:annotation>
          <xs:documentation source="valueID">10701</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Level off-touchdown</xs:documentation>
          <xs:documentation source="detailedDescription">Level off-touchdown.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the point of transition from nose-low to nose-up attitude, just before landing, until touchdown.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10702">
        <xs:annotation>
          <xs:documentation source="valueID">10702</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing roll</xs:documentation>
          <xs:documentation source="detailedDescription">Landing roll.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from touchdown until the aircraft exits the landing runway or comes to a stop, whichever occurs first.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990918">
        <xs:annotation>
          <xs:documentation source="valueID">990918</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing roll - Landing roll - on runway</xs:documentation>
          <xs:documentation source="detailedDescription">Landing roll - on runway</xs:documentation>
          <xs:documentation source="explanation">Landing roll - on runway: the part of the landing roll when the aircraft is on the runway.
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10708">
        <xs:annotation>
          <xs:documentation source="valueID">10708</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing roll - Landing roll - off runway</xs:documentation>
          <xs:documentation source="detailedDescription">Landing roll - off runway</xs:documentation>
          <xs:documentation source="explanation">Landing roll - off runway: the part of the landing roll after the aircraft left the runway by excursion to the side or overrun the end of the runway. Also to be used when the aircraft has landed beside, in front of (undershoot) or behind the runway. The intent is to assist in better qualifying the related events, in particular in regards to any damages sustained.

Added with 427 HF1 after April 2008. This implies that records compiled before that date do not contain this classification.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990920">
        <xs:annotation>
          <xs:documentation source="valueID">990920</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing aborted before touch-down</xs:documentation>
          <xs:documentation source="detailedDescription">Landing aborted before touch-down</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an attempt is made to get airborne before touchdown (successful or not).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10703">
        <xs:annotation>
          <xs:documentation source="valueID">10703</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing aborted after touch-down</xs:documentation>
          <xs:documentation source="detailedDescription">Landing aborted after touch-down.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which an attempt is made to get airborne after touchdown (successful or not). This does not include the take-off portion of a touch-and-go.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10706">
        <xs:annotation>
          <xs:documentation source="valueID">10706</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Emergency landing or off-runway landing</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency landing or off-runway landing.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight where an intentional landing is made at a point other than a runway or normal landing site, in response to an in-flight emergency.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10798">
        <xs:annotation>
          <xs:documentation source="valueID">10798</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Landing - Landing: Other</xs:documentation>
          <xs:documentation source="detailedDescription">Landing: Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of flight while landing other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10800">
        <xs:annotation>
          <xs:documentation source="valueID">10800</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Post-impact</xs:documentation>
          <xs:documentation source="detailedDescription">Post-impact.</xs:documentation>
          <xs:documentation source="explanation">The phase, in an accident or incident sequence, after the aircraft has collided with the first object, place or person.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="19900">
        <xs:annotation>
          <xs:documentation source="valueID">19900</xs:documentation>
          <xs:documentation source="description">Powered Fixed-wing aircraft - Unknown</xs:documentation>
          <xs:documentation source="detailedDescription">Unknown.</xs:documentation>
          <xs:documentation source="explanation">An event involving a phase of flight of the aircraft that is unknown or not recorded.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990824">
        <xs:annotation>
          <xs:documentation source="valueID">990824</xs:documentation>
          <xs:documentation source="description">Helicopter</xs:documentation>
          <xs:documentation source="detailedDescription">Helicopter.</xs:documentation>
          <xs:documentation source="explanation">This section covers flight phases specifically adopted for the operation of helicopters.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990835">
        <xs:annotation>
          <xs:documentation source="valueID">990835</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing</xs:documentation>
          <xs:documentation source="detailedDescription">Standing.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight prior to taxi, or after arrival, at the gate, ramp, landing or parking area, while the helicopter is stationary.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990844">
        <xs:annotation>
          <xs:documentation source="valueID">990844</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Engine(s) Not Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Not Operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when an helicopter is parked at the ramp, or parking area, during which no engine is running.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990845">
        <xs:annotation>
          <xs:documentation source="valueID">990845</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Engine(s) Start-up</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Start-up.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight, while the helicopter is parked during which the first engine is started.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990847">
        <xs:annotation>
          <xs:documentation source="valueID">990847</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Engine(s) Run-up</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engine(s) Run-up.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight after start-up, during which power is applied to engines, for a pre-flight engine performance test.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990846">
        <xs:annotation>
          <xs:documentation source="valueID">990846</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Engine(s) Operating</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Engines operating.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight following engine start-up, or after post-flight arrival at the destination.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10105">
        <xs:annotation>
          <xs:documentation source="valueID">10105</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Rotors turning</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Rotors turning.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight when the helicopter is standing with its rotors turning prior to take-off or after landing.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990848">
        <xs:annotation>
          <xs:documentation source="valueID">990848</xs:documentation>
          <xs:documentation source="description">Helicopter - Standing - Standing : Other</xs:documentation>
          <xs:documentation source="detailedDescription">Standing : Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any standing phase of flight other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990922">
        <xs:annotation>
          <xs:documentation source="valueID">990922</xs:documentation>
          <xs:documentation source="description">Helicopter - Tow</xs:documentation>
          <xs:documentation source="detailedDescription">Tow</xs:documentation>
          <xs:documentation source="explanation"></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990836">
        <xs:annotation>
          <xs:documentation source="valueID">990836</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi.</xs:documentation>
          <xs:documentation source="explanation">Taxiing includes air taxiing for rotorcraft.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990849">
        <xs:annotation>
          <xs:documentation source="valueID">990849</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Taxi to runway</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi to runway.</xs:documentation>
          <xs:documentation source="explanation">Commences when the helicopter begins to move leaving the gate, ramp, apron, or parking area, and terminates upon reaching the runway.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990850">
        <xs:annotation>
          <xs:documentation source="valueID">990850</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Taxi to take-off position</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi to take-off position.</xs:documentation>
          <xs:documentation source="explanation">From entering the runway until reaching the take-off position.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990851">
        <xs:annotation>
          <xs:documentation source="valueID">990851</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Taxi from runway</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi from runway.</xs:documentation>
          <xs:documentation source="explanation">Begins upon exiting the landing runway and terminates upon arrival at the gate, ramp, apron, or parking area.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990852">
        <xs:annotation>
          <xs:documentation source="valueID">990852</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Maintaining position</xs:documentation>
          <xs:documentation source="detailedDescription">Maintaining position.</xs:documentation>
          <xs:documentation source="explanation">Maintaining position at holding point.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10203">
        <xs:annotation>
          <xs:documentation source="valueID">10203</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Air taxi/hover taxi</xs:documentation>
          <xs:documentation source="detailedDescription">Air taxi/hover taxi.</xs:documentation>
          <xs:documentation source="explanation">Air-taxiing: movement of a helicopter above the surface of an aerodrome, normally in ground effect and at a ground speed normally less than 37 km/h (20 kt). ICAO Annex 2.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990853">
        <xs:annotation>
          <xs:documentation source="valueID">990853</xs:documentation>
          <xs:documentation source="description">Helicopter - Taxi - Taxi : Other</xs:documentation>
          <xs:documentation source="detailedDescription">Taxi : Other.</xs:documentation>
          <xs:documentation source="explanation">An event involving any phase of taxiing other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990837">
        <xs:annotation>
          <xs:documentation source="valueID">990837</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Take-off.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight from the application of take-off power until reaching the first prescribed power reduction, or until reaching the VFR pattern or 1000 feet (300 metres) above runway end elevation, whichever comes first or the termination (abort) of the take-off.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above the runway end elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990854">
        <xs:annotation>
          <xs:documentation source="valueID">990854</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Running take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Running take-off.</xs:documentation>
          <xs:documentation source="explanation">The take-off of an helicopter where the aircraft is accelerated on the surface until the speed is sufficient to establish a positive rate of climb condition.  </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="10307">
        <xs:annotation>
          <xs:documentation source="valueID">10307</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Vertical take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Vertical take-off.</xs:documentation>
          <xs:documentation source="explanation">Vertical-take-off for helicopters.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990860">
        <xs:annotation>
          <xs:documentation source="valueID">990860</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Vertical take-off with external load attached</xs:documentation>
          <xs:documentation source="detailedDescription">Vertical take-off with external load attached.</xs:documentation>
          <xs:documentation source="explanation">A vertical take-off where the helicopter has already an external load attached.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990856">
        <xs:annotation>
          <xs:documentation source="valueID">990856</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Rejected take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Rejected take-off.</xs:documentation>
          <xs:documentation source="explanation">During take-off, from the point where the decision to abort has been taken until the helicopter begins to taxi from the runway.

Landback during rotorcraft operations is considered a rejected takeoff.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990855">
        <xs:annotation>
          <xs:documentation source="valueID">990855</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Initial climb</xs:documentation>
          <xs:documentation source="detailedDescription">Initial climb.</xs:documentation>
          <xs:documentation source="explanation">From the end of the Take-off run sub-phase to the first prescribed power reduction, or until reaching 1000 feet above runway elevation or the VFR pattern, whichever comes first.

Note: this slightly differs from the previous definition where 1,500 feet (450 metres) above runway elevation was specified.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990857">
        <xs:annotation>
          <xs:documentation source="valueID">990857</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Climb into traffic pattern</xs:documentation>
          <xs:documentation source="detailedDescription">Climb into traffic pattern.</xs:documentation>
          <xs:documentation source="explanation"></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990858">
        <xs:annotation>
          <xs:documentation source="valueID">990858</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Emergency descent during take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Emergency descent during take-off.</xs:documentation>
          <xs:documentation source="explanation">A controlled descent during take-off phase in response to a perceived emergency situation.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990859">
        <xs:annotation>
          <xs:documentation source="valueID">990859</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Uncontrolled descent during take-off</xs:documentation>
          <xs:documentation source="detailedDescription">Uncontrolled descent during take-off.</xs:documentation>
          <xs:documentation source="explanation">A descent during take-off phase in which the helicopter does not sustain controlled flight.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990861">
        <xs:annotation>
          <xs:documentation source="valueID">990861</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Hovering/transition from hover to forward flight</xs:documentation>
          <xs:documentation source="detailedDescription">Hovering/transition from hover to forward flight.</xs:documentation>
          <xs:documentation source="explanation"></xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990862">
        <xs:annotation>
          <xs:documentation source="valueID">990862</xs:documentation>
          <xs:documentation source="description">Helicopter - Take-off - Take-off : Other</xs:documentation>
          <xs:documentation source="detailedDescription">Take-off : Other.</xs:documentation>
          <xs:documentation source="explanation">The phase of flight in which any event occurs during take-off, other than one of the above.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="990838">
        <xs:annotation>
          <xs:documentation source="valueID">990838</xs:documentation>
          <xs:documentation source="description">Helicopter - En-route</xs:documentation>
          <xs:documentation source="detailedDescription">En-route.</xs:documentation>
          <xs:documentation source="explanation"></xs:documentation>
        </xs:annotation>
